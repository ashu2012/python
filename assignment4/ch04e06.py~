def is_divisible_by_3 (num):
 	if ((num % 3)== 0):
		print "This number "+str(num)+" is divisible by three."
	else :
		print "This number "+str(num)+" is not divisible by three."

def is_divisible_by_5(num):
	if ((num % 5)== 0):
		print "This number "+str(num)+" is divisible by five."
	else: 
		print "This number "+str(num)+" is not divisible by five."


is_divisible_by_3 (81)
is_divisible_by_3 (51)
is_divisible_by_5 (90)
is_divisible_by_5 (91)
